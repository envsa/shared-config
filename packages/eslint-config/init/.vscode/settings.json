{
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "never"
  },
  "eslint.enable": true,
  "eslint.runtime": "node",
  "eslint.validate": [
    "astro",
    "html",
    "javascript",
    "json",
    "json5",
    "jsonc",
    "markdown",
    "svelte",
    "toml",
    "typescript",
    "twig",
    "xml",
    "yaml"
  ],
  "eslint.useFlatConfig": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true
}
