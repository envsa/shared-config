{
	"$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
	"enabled": true,
	"enableFiletypes": ["astro", "md", "mdx", "patch", "svelte"],
	"dictionaryDefinitions": [
		{
			"name": "kp-acronyms",
			"path": "./dictionaries/kp-acronyms.txt",
			"addWords": true
		},
		{
			"name": "kp-brands",
			"path": "./dictionaries/kp-brands.txt",
			"addWords": true
		},
		{
			"name": "kp-misc",
			"path": "./dictionaries/kp-misc.txt",
			"addWords": true
		},
		{
			"name": "kp-names",
			"path": "./dictionaries/kp-names.txt",
			"addWords": true
		},
		{
			"name": "kp-tech",
			"path": "./dictionaries/kp-tech.txt",
			"addWords": true
		}
	],
	"dictionaries": [
		"lorem-ipsum",
		"git",
		"gaming-terms",
		"npm",
		"data-science",
		"fullstack",
		"kp-acronyms",
		"kp-brands",
		"kp-misc",
		"kp-names",
		"kp-tech"
	],
	"languageSettings": [
		{
			"languageId": "markdown,mdx",
			"ignoreRegExpList": ["/^```(?:.|\\s)+?^```/mig"]
		}
	],
	"globRoot": "/",
	"ignorePaths": [
		"__snapshots__",
		"*.m4a",
		"*.mp3",
		"*.mp4",
		"*.svg",
		"*.tldr",
		"*.app",
		"*.scpt",
		"package.json",
		"patches/",
		"pnpm-lock.yaml"
	],
	"ignoreRegExpList": ["tp-.+", "tweakpane-plugin-.+", "v2_c_\\w{21}"],
	"language": "en,en-US",
	"useGitignore": true,
	"version": "0.2"
}
